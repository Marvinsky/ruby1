<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<ul>
  <% @posts.each do |post| %>
    <h3> <%= post.titulo %> </h3>
    <% if post.has_photo %>
      <%= image_tag post.photo_path, :width => 400, :height => 300, :crop => :fill %>
    <% end %>
  <% end %>
</ul>

<br>

<%= link_to 'New Post', new_post_path %>